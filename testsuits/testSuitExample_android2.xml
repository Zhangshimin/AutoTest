<?xml version="1.0" encoding="UTF-8"?>
<suite name="precodition">
	<!--platformName in {web ,android ,ios } -->
	<parameter
		name="platformName"
		value="Android" />
	<!--APP PARAM //adb devices 获取 -->
	<parameter
		name="appName"
		value="XiaonengChatDemo3.apk" />
	<parameter
		name="filePath"
		value="apps" />
	<parameter
		name="platformVersion"
		value="4.4.2" />
	<parameter
		name="deviceName"
		value="" />
	<parameter
		name="appPackage"
		value="com.example.androidsdk" />
	<parameter
		name="appActivity"
		value="com.example.androidsdk.MainActivity" />
	<parameter
		name="port"
		value="5743" />
	<parameter
		name="timeout"
		value="30" />
	<parameter
		name="udid"
		value="9TGA6TGIBEPJRS4T" />
	<!--WEB PARAM -->
	<parameter
		name="browser_name"
		value="chrome" />
	<parameter
		name="remote_url"
		value="www.baidu.com" />

	<!--enabled="true"让测试生效，可根据情况开启或关闭某些测试 preserve-order="true" -->
	<test name="APP_Tests2">
		<groups>
			<run>
				<include name="p0" />
			</run>
		</groups>
		<classes>
			<parameter
				name="test"
				value="test" />
			<class
				name="com.xiaoneng.uiautotest.testsuits.testcases.app.Example_Tests" />

		</classes>
	</test>

	<listeners>
		<listener class-name="com.xiaoneng.uiautotest.testng.TestResultListener" />
		<listener class-name="com.xiaoneng.uiautotest.testng.RetryListener" />
	</listeners>
</suite>